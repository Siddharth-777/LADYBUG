{
  "version": 2,
  "builds": [
    { "src": "api/app.py", "use": "@vercel/python", "config": { "pythonVersion": "3.11" } },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/chat", "dest": "api/app.py" },
    { "src": "/", "dest": "/static/index.html" },
    { "src": "/(.*)", "dest": "/static/$1" }
  ]
}
